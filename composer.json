{
    "name": "directorytree/ldaprecord-lumen",
    "description": "LDAP Authentication & Management for Lumen.",
    "keywords": ["ldaprecord", "adldap2", "ldap", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.2",
        "ext-ldap": "*",
        "directorytree/ldaprecord-laravel": "^v2.1.1",
        "laravel/lumen-framework": "^5.6|^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5"
    },
    "archive": {
        "exclude": ["/tests"]
    },
    "autoload": {
        "psr-4": {
            "LdapRecord\\Lumen\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LdapRecord\\Lumen\\Tests\\": "tests/"
        }
    }
}
